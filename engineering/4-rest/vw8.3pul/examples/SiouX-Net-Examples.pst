ponder' 		or: [aRequestLine decodedPath last = 'xtreams'])		ifTrue: [ ^false ].	^ (super acceptRequest: aRequestLine)</body><body package="SiouX-Net-Examples">executeRequestFor: aRequestContext	(aRequestContext request url tail = 'error') ifTrue: [^self error: (#RequestExecutionFailed &lt;&lt; #www &gt;&gt; 'Responder error ') ].	(aRequestContext request url tail = 'post') ifTrue: [		^aRequestContext response 			contentType:  'text/plain';			contents: aRequestContext request contents;			yourself].	^aRequestContext response 		contentType:  'text/html';		contents: '&lt;HTML&gt;&lt;BODY&gt;&lt;H1&gt;Hello World!&lt;/H1&gt;&lt;/BODY&gt;&lt;/HTML&gt;';		yourself</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>NetHttpResponder</name><environment>SiouX</environment><super>SiouX.Responder</super><private>false</private><indexed-type>none</indexed-type><inst-vars>readingOptions writingOptions </inst-vars><class-inst-vars>defaultReadingOptions defaultWritingOptions </class-inst-vars><imports>			private OS.IOConstants.*			private Net.*			</imports><category></category><attributes><package>SiouX-Net-Http</package></attributes></class></st-source>