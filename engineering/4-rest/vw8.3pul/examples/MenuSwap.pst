.	mb add: 'Add Above' -&gt; #unimplemented;		line;		add: 'Delete' -&gt; #unimplemented;		add: 'Delete All' -&gt; #unimplemented.	^mb menu</body><body package="MenuSwap-Example">nothingSelectedMenu	| mb |	mb := MenuBuilder new.	mb add: 'Add At Bottom' -&gt; #unimplemented;		line;		add: 'Delete All' -&gt; #unimplemented.	^mb menu</body></methods><methods><class-id>Examples.MenuSwapExample</class-id> <category>initialize-release</category><body package="MenuSwap-Example">initialize	colors := SelectionInList with: ColorValue constantNames.	colors selectionIndexHolder onChangeSend: #selectionChanged to: self.	menuHolder := self nothingSelectedMenu asValue.</body></methods><methods><class-id>Examples.MenuSwapExample</class-id> <category>change messages</category><body package="MenuSwap-Example">selectionChanged	self colors selection isNil		ifTrue: [self menuHolder value: self nothingSelectedMenu]		ifFalse: [self menuHolder value: self colorSelectedMenu]</body></methods><methods><class-id>Examples.MenuSwapExample</class-id> <category>actions</category><body package="MenuSwap-Example">unimplemented	^Dialog warn: 'The commands in this menu do nothing'</body></methods><methods><class-id>Examples.MenuSwapExample class</class-id> <category>interface specs</category><body package="MenuSwap-Example">windowSpec	"UIPainter new openOnClass: self andSelector: #windowSpec"	&lt;resource: #canvas&gt;	^#(#FullSpec 		#window: 		#(#WindowSpec 			#label: 'MenuSwapExample' 			#min: #(#Point 212 212 ) 			#max: #(#Point 250 212 ) 			#bounds: #(#Rectangle 720 477 970 689 ) ) 		#component: 		#(#SpecCollection 			#collection: #(				#(#SequenceViewSpec 					#layout: #(#LayoutFrame 14 0 10 0 -14 1 -50 1 ) 					#model: #colors 					#menu: #menuHolder ) 				#(#LabelSpec 					#layout: #(#AlignmentOrigin 0 0.5 -40 1 0.5 0 ) 					#label: 'When a list item is selected,' ) 				#(#LabelSpec 					#layout: #(#AlignmentOrigin 0 0.5 -25 1 0.5 0 ) 					#label: 'the pop-up menu changes' ) ) ) )</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>ApplicationModel</name><environment>UI</environment><super>UI.Model</super><private>false</private><indexed-type>none</indexed-type><inst-vars>builder uiSession eventHandlers </inst-vars><class-inst-vars>savedWindowInformation </class-inst-vars><imports></imports><category>UIBuilder-Framework</category><attributes><package>UIBuilder-Framework</package></attributes></class></st-source>