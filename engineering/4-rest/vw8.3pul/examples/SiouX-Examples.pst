ion expire]</body></methods><methods><class-id>SiouX.ServerLoadRule</class-id> <category>private</category><body package="SiouX-Examples">preferredAgressiveExpirationLevel	^self class expireLevel.</body></methods><methods><class-id>SiouX.ServerLoadRule</class-id> <category>initialize-release</category><body package="SiouX-Examples">initialize	super initialize.	thresholds := IdentityDictionary new.	self normalThreshold: self class normalThreshold. " 300. 5 minutes"	self stressedThreshold:  self class stressedThreshold. "150. 50% of normal: 2.5 minutes"</body></methods><methods><class-id>SiouX.ServerLoadRule</class-id> <category>accessing</category><body package="SiouX-Examples">normalThreshold	^thresholds at: 0</body><body package="SiouX-Examples">normalThreshold: aValue	thresholds at: 0 put: aValue</body><body package="SiouX-Examples">stressedThreshold	^thresholds at: (self preferredAgressiveExpirationLevel) ifAbsent: [ self normalThreshold ]</body><body package="SiouX-Examples">stressedThreshold: aValue	thresholds at: (self preferredAgressiveExpirationLevel) put: aValue</body></methods><methods><class-id>SiouX.ServerLoadRule class</class-id> <category>accessing</category><body package="SiouX-Examples">normalThreshold	^normalThreshold ifNil: [ self defaultNormalThresholdValue ]</body><body package="SiouX-Examples">stressedThreshold	^stressedThreshold ifNil: [ self defaultStressedThresholdValue ]</body></methods><methods><class-id>SiouX.ServerLoadRule class</class-id> <category>defaults</category><body package="SiouX-Examples">defaultNormalThresholdValue	^300</body><body package="SiouX-Examples">defaultStressedThresholdValue	^150</body></methods><methods><class-id>SiouX.ServerLoadRule class</class-id> <category>settings-web-accessing</category><body package="SiouX-Examples">expireLevel	^self memoryStressLevels indexOf: (self lowMemoryActionsDictionary at: #aggressiveSessionExpiration).</body><body package="SiouX-Examples">expireLevel: anInteger	^self lowMemoryActionsDictionary 		at: #aggressiveSessionExpiration 		put: (self memoryStressLevels at: anInteger)</body><body package="SiouX-Examples">initializeLowMemoryActionsDictionary	lowMemoryActionsDictionary := Dictionary new.	lowMemoryActionsDictionary at: #purgeCachedResources put: #minimum.	lowMemoryActionsDictionary at: #aggressiveSessionExpiration put: #light.	lowMemoryActionsDictionary at: #deferLaunchRequests put: #medium.	lowMemoryActionsDictionary at: #deferSubmits put: #heavy.</body><body package="SiouX-Examples">lowMemoryActionsDictionary	lowMemoryActionsDictionary isNil 		ifTrue: [self initializeLowMemoryActionsDictionary].	^lowMemoryActionsDictionary.</body><body package="SiouX-Examples">memoryStressLevels	^#(minimum light medium heavy maximum).</body></methods><methods><class-id>SiouX.ServerLoadRule class</class-id> <category>instance creation</category><body package="SiouX-Examples">new	^super new initialize</body><body package="SiouX-Examples">newMinimum	^self new.</body></methods><methods><class-id>SiouX.WebDevGuideResponder</class-id> <category>accessing</category><body package="SiouX-Examples">contentDispositionType	^'inline'</body><body package="SiouX-Examples">rootDirectory	^rootDirectory ifNil: [rootDirectory := (OS.LogicalFilename fromComponents: (Array with: '$(VISUALWORKS)' with: 'doc'))]</body></methods><methods><class-id>SiouX.Hello</class-id> <category>services</category><body package="SiouX-Examples">acceptRequest: aRequestLine	aRequestLine decodedPath last = 'noresponder' ifTrue: [ ^false].	^ (super acceptRequest: aRequestLine)</body><body package="SiouX-Examples">executeRequestFor: aRequestContext	(aRequestContext request url tail = 'error') ifTrue: [^self error: (#ResponderError &lt;&lt; #www &gt;&gt; 'Responder error ')].	(aRequestContext request url tail = 'xtreams') ifTrue: [		^aRequestContext  response			contentType:  'text/html';			contents: '&lt;HTML&gt;&lt;BODY&gt;&lt;H1&gt;xtreams&lt;/H1&gt;&lt;/BODY&gt;&lt;/HTML&gt;';			yourself].	(aRequestContext request url tail = 'post') ifTrue: [		^aRequestContext  response 			contentType:  'text/plain';			contents: ((aRequestContext request getFormData data) ifNotNil: [ :data | data first value]);			yourself].	^aRequestContext   response 		contentType:  'text/html';		contents: '&lt;HTML&gt;&lt;BODY&gt;&lt;H1&gt;Hello World!&lt;/H1&gt;&lt;/BODY&gt;&lt;/HTML&gt;';		yourself</body></methods><methods><class-id>SiouX.WelcomeResponder</class-id> <category>private</category><body package="SiouX-Examples">createHtmlFor: pathToken	| bodyStream stream title|	title := (pathToken = 'responders'		ifTrue: ['Currently loaded Responders' ]		ifFalse: [ 'Welcome to SiouX' ]).	stream := ByteArray new writing.	bodyStream := stream encoding: #'utf-8'.				bodyStream		write: '&lt;!DOCTYPE HTML&gt;&lt;html&gt;&lt;head&gt;	&lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt;	&lt;title&gt;';		write: title;		write: '&lt;/title&gt;';		cr;		write: '&lt;style&gt;';		write: self class commonCss;		cr;		write: '&lt;/style&gt;';		cr;		write: '&lt;/head&gt;&lt;body&gt;	&lt;div class="content"&gt;	&lt;a href="http://www.cincomsmalltalk.com/" title="Cincom Smalltalk Web Site" target="_blank"&gt;		&lt;img src="';		write: self class cincomSmalltalkImage;		write: '" alt="Cincom Smalltalk"&gt;	&lt;/a&gt;	&lt;nav&gt;		&lt;a href="/"&gt;Home&lt;/a&gt; |		&lt;a href="/responders"&gt;Responders&lt;/a&gt;'.	"construct the string below with full quotes around # and the onclick function"	bodyStream write: ' | &lt;a href= "documentation/WebServerGuide.pdf" target="_blank"&gt;Documentation&lt;/a&gt; '.	bodyStream 		write: '	&lt;/nav&gt;	&lt;h1&gt;';		write: title;		write: '&lt;/h1&gt;';		cr.		pathToken = 'responders'		ifTrue: [ self respondersContents: bodyStream]		ifFalse: [ self welcomeContents: bodyStream].	bodyStream 		cr;		write:'	&lt;/div&gt;&lt;/body&gt;&lt;/html&gt;'.	bodyStream close.	^stream contents</body><body package="SiouX-Examples">respondersContents: stream	stream		write: '&lt;style&gt;';		write: self class applicationsCss;		cr;		write: '&lt;/style&gt;';		write: '&lt;table&gt;';		cr.	((server responders reject: [:r | 			r == self 				or: [r class name = #FaviconResponder 					or:[ r class name = #WebDevGuideResponder]]]) 		sorted: [ :a :b | a class name &lt;= b class name ]) 		do: [ :each |			stream write: '&lt;tr class="responder"&gt;'.			stream				cr;				write: '&lt;td class="application"&gt;';				write: '&lt;a target="_blank" href="';				write: each path;				write: '"&gt;';				write: each class name;				write: '&lt;/a&gt;';				write: '&lt;/td&gt;';				cr;				write: '&lt;td class="comment"&gt;';				write: (each class comment readStream upTo: Character cr);				cr;				write: '&lt;/td&gt;';				cr;				write: '&lt;/tr&gt;' ].	stream		cr;		write: '&lt;/table&gt;'</body><body package="SiouX-Examples">welcomeContents: aStream	aStream write: 	'&lt;p&gt;Cincom Smalltalk&amp;reg; provides an integrated HTTP server framework named SiouX. The SiouX package contains a lightweight, scalable HTTP server.&lt;/p&gt;	&lt;h3&gt;Features&lt;/h3&gt;	&lt;ul&gt;		&lt;li&gt;Support for HTTP and HTTPS protocols&lt;/li&gt;		&lt;li&gt;Compatible with current and legacy Smalltalk server frameworks - Seaside, Web Services, Servlets, NetClients, etc.&lt;/li&gt;		&lt;li&gt;Supports WebSocket connections for a full duplex client / server communications&lt;/li&gt;		&lt;li&gt;Full access to the socket stream for a fast and memory efficient streaming responses&lt;/li&gt;		&lt;li&gt;Pluggable filters allow request pre-processing for localization, protocol upgrades, session tracking, etc.&lt;/li&gt;		&lt;li&gt;Configurable session caching&lt;/li&gt;		&lt;li&gt;Common HTTP Logging&lt;/li&gt;		&lt;li&gt;Configuration console accessible from VisualWorks launcher window&lt;/li&gt;		&lt;li&gt;Web based Server Monitor tool available as part of AppeX, our new web application development framework&lt;/li&gt;	&lt;/ul&gt;'.</body></methods><methods><class-id>SiouX.WelcomeResponder</class-id> <category>services</category><body package="SiouX-Examples">executeRequestFor: aRequestContext"The method returns responses:	HTML pages for an empty path, 	list of responders for /responders,	Web Server pdf file for /doc"	|  reqPath |	reqPath := aRequestContext request url decodedPath allButFirst: 1.		reqPath size = 1 ifTrue: [		reqPath := reqPath first.		reqPath = '' ifTrue: [^self replyHomePage: aRequestContext].		reqPath = 'responders' ifTrue: [ ^self replyRespondersPage: aRequestContext].		].	(aRequestContext response) 		code: 404; 		contentType: 'text/html';		contents:  '&lt;p&gt;The requested document was not found in this responder.&lt;/p&gt;'</body><body package="SiouX-Examples">replyHomePage: aRequestContext	aRequestContext response		contentType: 'text/html';		contents: (self createHtmlFor: '')</body><body package="SiouX-Examples">replyRespondersPage: aRequestContext	aRequestContext response		contentType: 'text/html';		contents: (self createHtmlFor: 'responders')</body></methods><methods><class-id>SiouX.WelcomeResponder class</class-id> <category>initialize-release</category><body package="SiouX-Examples">obsolete	self stopExampleServer</body></methods><methods><class-id>SiouX.WelcomeResponder class</class-id> <category>content</category><body package="SiouX-Examples">applicationsCss	^ 'pre {	font-family: Verdana, Geneva, sans-serif;}.responder {	text-align: left;}.responder td {	vertical-align: top;	border: thin dotted lightgrey;	padding: 0.2em 0.5em;}.application {	min-width: 20em;}.application, .comment pre {	margin: 0px;}.comment {	font-size: small;	white-space: pre-wrap;}'</body><body package="SiouX-Examples">cincomSmalltalkImage	"	The image data is generated by using an online tool at https://websemantics.uk/tools/image-to-data-uri-converter/.	Downloaded the Cincom Smalltalk banner image from http://www.cincomsmalltalk.com/main/ .	The URL of the image itself is http://www.cincomsmalltalk.com/main/wp-content/uploads/2014/10/CST-Web-Header.png	but we should not count in this address to remain constant in future.	Once the image is downloaded, the image was converted to png format, white was selected as the transparent background color, 	and the image to data convertor was used to generate this string.	"	^ 'data:image/png;base64,'</body><body package="SiouX-Examples">commonCss	^'html {	background-color: #e7e7e7;}body {	background-color: white;	max-width: 960px;	text-align: center;	font-family: sans-serif;	padding: 1em;	box-shadow: 0px 1px 15px #aaaaaa;	margin: 2em auto;}.content {	text-align: left;}nav {	text-align: left;	margin-top: 1em;}h1 {	text-align: center;}'</body></methods><methods><class-id>SiouX.WelcomeResponder class</class-id> <category>class initialization</category><body package="SiouX-Examples">stopExampleServer"	(self stopExampleServer)"	Server registry at: 'SiouXExamples' ifPresent: [ :server | server release ]</body></methods><methods><class-id>SiouX.SessionQueryFilter</class-id> <category>accessing</category><body package="SiouX-Examples">defaultVariableName	^ 'sessionId'</body><body package="SiouX-Examples">variableName	^variableName ifNil: [ self defaultVariableName ]</body><body package="SiouX-Examples">variableName: aString	variableName := aString</body></methods><methods><class-id>SiouX.SessionQueryFilter</class-id> <category>services</category><body package="SiouX-Examples">extractSessionIdFrom: aRequestContext	^(aRequestContext request queryData at: self variableName) first.</body></methods><methods><class-id>SiouX.ServerMemoryPolicy</class-id> <category>accessing</category><body package="SiouX-Examples">currentLoad	| potentialLoad actualLoad |	potentialLoad := self currentLoadNoGC.	potentialLoad &lt;= lastLoadLevel ifTrue: [^potentialLoad].	ObjectMemory quickGC.	actualLoad := self currentLoadNoGC.	actualLoad &gt; lastLoadLevel ifTrue: [self newLoadLevel: actualLoad].	^actualLoad</body><body package="SiouX-Examples">loadThresholds	^loadThresholds</body><body package="SiouX-Examples">loadThresholds: aValue 	loadThresholds := aValue</body><body package="SiouX-Examples">memoryLowerBound	^memoryLowerBound</body><body package="SiouX-Examples">memoryLowerBound: aValue 	memoryLowerBound := aValue</body></methods><methods><class-id>SiouX.ServerMemoryPolicy</class-id> <category>private</category><body package="SiouX-Examples">calculateLoadLevel	| percentUsed |	percentUsed := self calculateLoadPercent.	^(self class unloaded to: self class maximumLoad)		detect: [:loadLevel | percentUsed &lt; (loadThresholds at: loadLevel + 1)]		ifNone: [self class maximumLoad]</body><body package="SiouX-Examples">calculateLoadPercent	^100.0 * (ObjectMemory dynamicallyAllocatedFootprint - memoryStatus availableFreeOldSpaceBytes) / memoryUpperBound</body><body package="SiouX-Examples">currentLoadNoGC	| potentialLoad |	potentialLoad := self calculateLoadLevel.	potentialLoad = lastLoadLevel ifTrue: [^potentialLoad].	potentialLoad &lt; lastLoadLevel		ifTrue: 			[self newLoadLevel: potentialLoad.			^potentialLoad].	^potentialLoad</body><body package="SiouX-Examples">emergencyLowSpaceAction	"We are almost completely out of memory. Behave appropriately. There have been a lot of issues with fragility of older versions of this, so try to keep it extremely minimal.  - log something first, so that if we can't successfully do anything else, there will be some evidence what happened (as long as the Transcript is being logged).  - notify ObjectMemory dependents. This allows user code to take its own action to try and fix the situation. Uses both changed: and triggerEvent: for good measure. Allow user actions to veto subsequent actions if they think they've addressed the situation.  - if we are in a development image, and headfull, then do the default action, which is to pop up a processor monitor, thus suspending all user processes  - if we are either headfull, or in production, log the information that we're out of memory, then quit the image"		Transcript		cr;		show: '------------------------------------------------';		cr;		show: 'MEMORY EMERGENCY';		flush.	[ObjectMemory changed: #outOfMemory.	ObjectMemory triggerEvent: #outOfMemory] 		on: VetoAction do: [:ex | ^self]."	(WindowingSystem isHeadless not		and: [ProcessEnvironment isDevelopingOverride])		ifTrue: [^super emergencyLowSpaceAction]."	Transcript		cr;		show: '------------------------------------------------';		cr;		show: 'MEMORY EMERGENCY: Exiting';		flush.	ObjectMemory quit.</body><body package="SiouX-Examples">initialize	super initialize.	growthRegimeUpperBound := memoryUpperBound * 2 // 3.	preferredGrowthIncrement := memoryUpperBound - (memoryLowerBound ifNil: [0]) // 8.	growthRetryDecrement := preferredGrowthIncrement // 50.	freeMemoryUpperBound := memoryUpperBound // 5 max: preferredGrowthIncrement * 3.	incrementalAllocationThreshold := self preferredGrowthIncrement // 4.	idleLoopAllocationThreshold := self preferredGrowthIncrement * 5 // 2.	loadThresholds := #(80 90 94 97 99 100) copy.	memoryLowerBound := nil.	lastLoadLevel := 0.	self installPreferredSizes</body><body package="SiouX-Examples">installPreferredSizes	(ObjectMemory sizesAtStartup allSatisfy: [:each | each = 1.0]) ifTrue: [		"Only set the sizes if they've not been set elsewhere"		ObjectMemory sizesAtStartup: #(10.0 10.0 1.0 nil nil 5.0 nil)].</body><body package="SiouX-Examples">isHeadfull	^(OS.Smalltalk at: #HeadlessImage ifAbsent: [^true]) default isHeadless not.</body><body package="SiouX-Examples">isUsefulToBringUpADebuggerInLowSpaceEmergency	"In an emergency low space condition, should we bring up a debugger or just exit""	^ProcessEnvironment isDeveloping.""	^(Smalltalk at: #HeadlessImage ifAbsent: [^true]) default isHeadless not."	^true</body><body package="SiouX-Examples">myDependents	"Answer the receiver's dependents or nil."	^dependents</body><body package="SiouX-Examples">myDependents: dependentsOrNil 	"Set the receivers dependents."	dependents := dependentsOrNil</body><body package="SiouX-Examples">newLoadLevel: anInteger 	self changed: #currentLoad with: anInteger.	lastLoadLevel := anInteger</body><body package="SiouX-Examples">pregrowIfNeeded	| neededAmount |	memoryLowerBound isNil ifTrue: [^self].	neededAmount := memoryLowerBound - ObjectMemory dynamicallyAllocatedFootprint.	ObjectMemory growMemoryBy: neededAmount</body><body package="SiouX-Examples">resumeWindowManagerProcesses	self isHeadfull		ifTrue:			["Resume window procs to keep the system responsive"			UI.WindowManager allInstances 				do:					[:each | 					each windowProcess isNil						ifFalse: [[each windowProcess resume] on: Error do: [:ex | ]]]	"In case this is a runaway foreground process from a window we could try to interrupt the running one of those. But it doesn't seem to actually do much good"	"DebuggerService interruptActiveUserProcess"].</body></methods><methods><class-id>SiouX.ServerMemoryPolicy</class-id> <category>updating</category><body package="SiouX-Examples">update: anAspect with: arguments from: anObject 	"Check for return from snapshot to pre-grow memory and re-install from WebSettings."	anObject == ObjectMemory		ifTrue: [anAspect == #earlySystemInstallation ifTrue: [self pregrowIfNeeded]]</body></methods><methods><class-id>SiouX.ServerMemoryPolicy</class-id> <category>copying</category><body package="SiouX-Examples">postCopy	"Do not copy the dependents list."	super postCopy.	self breakDependents</body></methods><methods>