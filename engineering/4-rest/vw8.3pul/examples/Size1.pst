></methods><methods><class-id>Examples.Size1Example class</class-id> <category>interface specs</category><body package="Size1-Example">windowSpec	"UIPainter new openOnClass: self andSelector: #windowSpec"	&lt;resource: #canvas&gt;	^#(#FullSpec 		#window: 		#(#WindowSpec 			#label: 'Size1Example' 			#min: #(#Point 592 205 ) 			#bounds: #(#Rectangle 337 384 929 589 ) ) 		#component: 		#(#SpecCollection 			#collection: #(				#(#SequenceViewSpec 					#layout: #(#Rectangle 50 50 100 100 ) 					#flags: 12 					#model: #numbers ) 				#(#SequenceViewSpec 					#layout: #(#LayoutFrame 150 0 50 0 0 0.5 -90 1 ) 					#flags: 12 					#model: #numbers ) 				#(#SequenceViewSpec 					#layout: #(#LayoutFrame 50 0.5 50 0 100 0.5 100 0 ) 					#flags: 12 					#model: #numbers ) 				#(#SequenceViewSpec 					#layout: #(#LayoutFrame 150 0.5 50 0 -50 1 -90 1 ) 					#flags: 12 					#model: #numbers ) 				#(#LabelSpec 					#layout: #(#Point 50 13 ) 					#label: 'Fixed origin' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#Point 50 26 ) 					#label: 'Fixed size' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#Point 150 13 ) 					#label: 'Fixed origin' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#Point 150 26 ) 					#label: 'Relative size' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#LayoutOrigin 50 0.5 13 0 ) 					#name: #'Fixed size' 					#label: 'Rel. origin' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#LayoutOrigin 50 0.5 26 0 ) 					#label: 'Fixed size' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#LayoutOrigin 150 0.5 13 0 ) 					#name: #'Fixed size' 					#label: 'Relative origin' 					#style: #small ) 				#(#LabelSpec 					#layout: #(#LayoutOrigin 150 0.5 26 0 ) 					#label: 'Relative size' 					#style: #small ) 				#(#CompositeSpecCollection 					#collection: #(						#(#LabelSpec 							#layout: #(#LayoutOrigin 1 0 0 0 ) 							#label: 'Left' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 10 0 ) 							#label: 'Top' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 22 0 ) 							#label: 'Right' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 33 0 ) 							#label: 'Bottom' 							#style: #small ) ) 					#compositeSpec: 					#(#CompositeSpec 						#layout: #(#LayoutFrame 5 0 -64 1 59 0 -7 1 ) ) ) 				#(#GroupBoxSpec 					#layout: #(#LayoutFrame 2 0 -90 1 -3 1 -1 1 ) 					#label: 'Position Tool settings (proportion, offset)' ) 				#(#CompositeSpecCollection 					#collection: #(						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 33 0 ) 							#label: '  0   100' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 0 0 ) 							#label: '  0    50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 11 0 ) 							#label: '  0    50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 22 0 ) 							#label: '  0   100' 							#style: #small ) ) 					#compositeSpec: 					#(#CompositeSpec 						#layout: #(#LayoutFrame 65 0 -64 1 136 0 -5 1 ) ) ) 				#(#CompositeSpecCollection 					#collection: #(						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 0 0 ) 							#label: '  0   150' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 11 0 ) 							#label: '  0    50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 22 0 ) 							#label: '0.5     0' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 33 0 ) 							#label: '  1   -90' 							#style: #small ) ) 					#compositeSpec: 					#(#CompositeSpec 						#layout: #(#LayoutFrame 150 0 -64 1 211 0 -11 1 ) ) ) 				#(#CompositeSpecCollection 					#collection: #(						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 33 0 ) 							#label: '  0   100' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 0 0 ) 							#label: '0.5   50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 11 0 ) 							#label: '  0    50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 22 0 ) 							#label: '0.5   100' 							#style: #small ) ) 					#compositeSpec: 					#(#CompositeSpec 						#layout: #(#LayoutFrame 50 0.5 -64 1 116 0.5 -11 1 ) ) ) 				#(#CompositeSpecCollection 					#collection: #(						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 0 0 ) 							#label: '0.5   150' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 11 0 ) 							#label: '  0     50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 22 0 ) 							#label: '  1   -50' 							#style: #small ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 33 0 ) 							#label: '  1   -90' 							#style: #small ) ) 					#compositeSpec: 					#(#CompositeSpec 						#layout: #(#LayoutFrame 150 0.5 -64 1 222 0.5 -11 1 ) ) ) ) ) )</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>ApplicationModel</name><environment>UI</environment><super>UI.Model</super><private>false</private><indexed-type>none</indexed-type><inst-vars>builder uiSession eventHandlers </inst-vars><class-inst-vars>savedWindowInformation </class-inst-vars><imports></imports><category>UIBuilder-Framework</category><attributes><package>UIBuilder-Framework</package></attributes></class></st-source>