kage="Move-Example">moveArrowUp	| wrapper |	wrapper := (self builder componentAt: #arrow).	"If the arrow is not too high, raise it another notch."	wrapper bounds origin y &gt; 30		ifTrue: [wrapper moveBy: 0 @ -5]</body><body package="Move-Example">resetArrow	| wrapper |	wrapper := (self builder componentAt: #arrow).	wrapper moveTo: self arrowOrigin</body></methods><methods><class-id>Examples.MoveExample class</class-id> <category>resources</category><body package="Move-Example">arrowIcon	"UIMaskEditor new openOnClass: self andSelector: #arrowIcon"	&lt;resource: #image&gt;	^Image 		extent: 17@10 		depth: 1 		palette: CoveragePalette monoMaskPalette 		bits: #[0 128 0 0 0 96 0 0 0 56 0 0 255 254 0 0 			255 255 128 0 255 255 128 0 255 254 0 0 0 56 0 0 			0 96 0 0 0 128 0 0 ] pad: 32</body><body package="Move-Example">meterStick	"UIMaskEditor new openOnClass: self andSelector: #meterStick"	&lt;resource: #image&gt;	^Image 		extent: 5@91 		depth: 1 		palette: CoveragePalette monoMaskPalette 		bits: (ByteArray fromPackedString: '&gt;@@@@B@@@@@ @@@@H@@@@B@@@@A0@@@@H@@@@B@@@@@ @@@@H@@@@O @@@@ @@@@H@@@@B@@@@@ @@@@\@@@@B@@@@@ @@@@H@@@@B@@@@C8@@@@H@@@@B@@@@@ @@@@H@@@@G@@@@@ @@@@H@@@@B@@@@@ @@@@&gt;@@@@B@@@@@ @@@@H@@@@B@@@@A0@@@@H@@@@B@@@@@ @@@@H@@@@O @@@@ @@@@H@@@@B@@@@@ @@@@\@@@@B@@@@@ @@@@H@@@@B@@@@C8@@@@H@@@@B@@@@@ @@@@H@@@@G@@@@@ @@@@H@@@@B@@@@@ @@@@&gt;@@@@B@@@@@ @@@@H@@@@B@@@@A0@@@@H@@@@B@@@@@ @@@@H@@@@O @@@@ @@@@H@@@@B@@@@@ @@@@\@@@@B@@@@@ @@@@H@@@@B@@@@C8@@@@H@@@@B@@@@@ @@@@H@@@@G@@@@@ @@@@H@@@@B@@@@@ @@@@&gt;@@@@@@a') pad: 32</body></methods><methods><class-id>Examples.MoveExample class</class-id> <category>interface specs</category><body package="Move-Example">windowSpec	"UIPainter new openOnClass: self andSelector: #windowSpec"	&lt;resource: #canvas&gt;	^#(#FullSpec 		#window: 		#(#WindowSpec 			#label: 'MoveExample' 			#min: #(#Point 161 340 ) 			#max: #(#Point 161 340 ) 			#bounds: #(#Rectangle 479 334 640 674 ) ) 		#component: 		#(#SpecCollection 			#collection: #(				#(#LabelSpec 					#layout: #(#Point 49 149 ) 					#name: #arrow 					#label: #arrowIcon 					#hasCharacterOrientedLabel: false ) 				#(#CompositeSpecCollection 					#collection: #(						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 90 0 ) 							#label: #meterStick 							#hasCharacterOrientedLabel: false ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 0 0 ) 							#label: #meterStick 							#hasCharacterOrientedLabel: false ) 						#(#LabelSpec 							#layout: #(#LayoutOrigin 0 0 180 0 ) 							#label: #meterStick 							#hasCharacterOrientedLabel: false ) ) 					#compositeSpec: 					#(#CompositeSpec 						#layout: #(#Rectangle 72 33 81 308 ) ) ) 				#(#ActionButtonSpec 					#layout: #(#Rectangle 52 6 101 26 ) 					#model: #moveArrowUp 					#label: 'Up' 					#defaultable: true ) 				#(#ActionButtonSpec 					#layout: #(#Rectangle 52 314 101 334 ) 					#model: #moveArrowDown 					#label: 'Down' 					#defaultable: true ) 				#(#ActionButtonSpec 					#layout: #(#Rectangle 101 145 150 165 ) 					#model: #resetArrow 					#label: 'Reset' 					#defaultable: true ) ) ) )</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>ApplicationModel</name><environment>UI</environment><super>UI.Model</super><private>false</private><indexed-type>none</indexed-type><inst-vars>builder uiSession eventHandlers </inst-vars><class-inst-vars>savedWindowInformation </class-inst-vars><imports></imports><category>UIBuilder-Framework</category><attributes><package>UIBuilder-Framework</package></attributes></class></st-source>