lue	^self new handle: (LibPango				pango_attr_foreground_new: (aColorValue red * 16rFFFF) rounded				with: (aColorValue green * 16rFFFF) rounded				with: (aColorValue blue * 16rFFFF) rounded)</body><body package="Pango">gravity: aGravityCode	^self new handle: (LibPango pango_attr_gravity_new: aGravityCode)</body><body package="Pango">gravityHint: aGravityHintCode	^self new handle: (LibPango pango_attr_gravity_hint_new: aGravityHintCode)</body><body package="Pango">letterSpacing: aLetterSpacing	^self new handle: (LibPango				pango_attr_letter_spacing_new: aLetterSpacing toPangoScale)</body><body package="Pango">pixelSize: aPixelSize	^self new		handle: (LibPango pango_attr_size_new_absolute: aPixelSize toPangoScale)</body><body package="Pango">pointSize: aPointSize	^self new handle: (LibPango pango_attr_size_new: aPointSize toPangoScale)</body><body package="Pango">rise: aRise	^self new handle: (LibPango pango_attr_rise_new: aRise toPangoScale)</body><body package="Pango">scale: aScaleFactor	^self new handle: (LibPango pango_attr_scale_new: aScaleFactor asDouble)</body><body package="Pango">shapeBlock: aBlock ink: anInkRectangle logical: aLogicalRectangle	^ShapeAttribute block: aBlock ink: anInkRectangle logical: aLogicalRectangle</body><body package="Pango">stretch: aStretchCode	^self new handle: (LibPango pango_attr_stretch_new: aStretchCode)</body><body package="Pango">strikethrough: aBoolean	^self new		handle: (LibPango pango_attr_strikethrough_new: aBoolean coerceToCInteger)</body><body package="Pango">strikethroughColor: aColorValue	^self new handle: (LibPango				pango_attr_strikethrough_color_new: (aColorValue red * 16rFFFF) rounded				with: (aColorValue green * 16rFFFF) rounded				with: (aColorValue blue * 16rFFFF) rounded)</body><body package="Pango">style: aStyleCode	^self new handle: (LibPango pango_attr_style_new: aStyleCode)</body><body package="Pango">underline: anUnderlineCode	^self new handle: (LibPango pango_attr_underline_new: anUnderlineCode)</body><body package="Pango">underlineColor: aColorValue	^self new handle: (LibPango				pango_attr_underline_color_new: (aColorValue red * 16rFFFF) rounded				with: (aColorValue green * 16rFFFF) rounded				with: (aColorValue blue * 16rFFFF) rounded)</body><body package="Pango">variant: aVariantCode	^self new handle: (LibPango pango_attr_variant_new: aVariantCode)</body><body package="Pango">weight: aWeightCode	^self new handle: (LibPango pango_attr_weight_new: aWeightCode)</body></methods><methods><class-id>Pango.Attribute class</class-id> <category>compatibility</category><body package="Pango">allFromEmphasis: anObject	anObject isSymbol		ifTrue: 			[#bold == anObject ifTrue: [^Array with: (self weight: Weight bold)].			#italic == anObject ifTrue: [^Array with: (self style: Style italic)].			#underline == anObject				ifTrue: [^Array with: (self underline: Underline single)].			#large == anObject ifTrue: [^Array with: (self scale: Scale large)].			#small == anObject ifTrue: [^Array with: (self scale: Scale small)].			#subscript == anObject ifTrue: [^Array with: (self rise: -5)].			#superscript == anObject ifTrue: [^Array with: (self rise: 5)].			#jaggedEdge == anObject				ifTrue: [^Array with: (self underline: Underline error)].			#strikeout == anObject ifTrue: [^Array with: (self strikethrough: true)].			CharacterAttributes.DefaultAttributes at: anObject				ifPresent: 					[:block |					(block copiedValues isKindOf: StyleDescription)						ifTrue: [^self allFromEmphasis: block copiedValues emphasis]]].	anObject isSequenceable		ifTrue: 			[^anObject inject: Array new				into: [:accum :each | accum , (self allFromEmphasis: each)]].	(anObject isKindOf: Association)		ifTrue: 			[anObject key == #color				ifTrue: [^Array with: (self foregroundColor: anObject value)].			anObject key == #backColor				ifTrue: [^Array with: (self backgroundColor: anObject value)].			anObject key == #family				ifTrue: [^Array with: (self family: anObject value)].			anObject key == #pixelSize				ifTrue: [^Array with: (self pixelSize: anObject value)]].	^Array new</body></methods><methods><class-id>Pango.ShapeAttribute</class-id> <category>shapes</category><body package="Pango">dataOffset	"typedef struct {  PangoAttribute attr; 0, 1, 2  PangoRectangle ink_rect; 3, 4, 5, 6  PangoRectangle logical_rect; 7, 8 , 9, 10  gpointer              data; _11_  PangoAttrDataCopyFunc copy_func;  GDestroyNotify        destroy_func;} PangoAttrShape"	^11</body><body package="Pango">inkExtents	"typedef struct {  PangoAttribute attr; 0, 1, 2  PangoRectangle ink_rect; 3, 4, 5, 6  PangoRectangle logical_rect; 7, 8 , 9, 10  gpointer              data; _11_  PangoAttrDataCopyFunc copy_func;  GDestroyNotify        destroy_func;} PangoAttrShape"	| pointer |	pointer := CIntegerType int newPointerToAddress: handle.	^(pointer at: 3) fromPangoScale @ (pointer at: 4) fromPangoScale		extent: (pointer at: 5) fromPangoScale @ (pointer at: 6) fromPangoScale</body><body package="Pango">logicalExtents	"typedef struct {  PangoAttribute attr; 0, 1, 2  PangoRectangle ink_rect; 3, 4, 5, 6  PangoRectangle logical_rect; 7, 8 , 9, 10  gpointer              data; _11_  PangoAttrDataCopyFunc copy_func;  GDestroyNotify        destroy_func;} PangoAttrShape"	| pointer |	pointer := CIntegerType int newPointerToAddress: handle.	^(pointer at: 7) fromPangoScale @ (pointer at: 8) fromPangoScale		extent: (pointer at: 9) fromPangoScale @ (pointer at: 10) fromPangoScale</body><body package="Pango">shapeBlock	^self class registeredBlockAt: (self pointer at: self dataOffset)</body><body package="Pango">shapeBlock: aSmalltalkBlock	^self pointer at: self dataOffset		put: (self class registerBlock: aSmalltalkBlock)</body></methods><methods><class-id>Pango.ShapeAttribute class</class-id> <category>block management</category><body package="Pango">nextIndex	"self nextIndex"	^LastIndex := LastIndex + 1 bitAnd: 16rFFFFFFF</body><body package="Pango">registerBlock: aBlock	^Lock critical: 			[| newIndex |			newIndex := self nextIndex.			RegisteredBlocks at: newIndex put: 1 -&gt; aBlock.			newIndex]</body><body package="Pango">registeredBlockAt: anIndex	^Lock critical: [(RegisteredBlocks at: anIndex ifAbsent: []) value]</body><body package="Pango">reregisterBlockAt: anIndex	^Lock critical: 			[RegisteredBlocks at: anIndex ifPresent: [:tuple | tuple key: tuple key + 1]]</body><body package="Pango">unregisterBlockAt: anIndex	^Lock critical: 			[RegisteredBlocks at: anIndex				ifPresent: 					[:tuple |					tuple key &lt; 2						ifTrue: [RegisteredBlocks removeKey: anIndex]						ifFalse: [tuple key: tuple key - 1]]]</body></methods><methods><class-id>Pango.ShapeAttribute class</class-id> <category>callbacks</category><body package="Pango">dataCopyCallback	DataCopyCallback ifNil: [self initializeCallbacks].	^DataCopyCallback</body><body package="Pango">destroyNotificationCallback	DestroyNotifyCallback ifNil: [self initializeCallbacks].	^DestroyNotifyCallback</body><body package="Pango">initializeCallbacks	"self initializeCallbacks"	ShapeRendererCallback := CCallback do: 					[:crAddress :attributeAddress :doPath :ignored |					| attribute |					attribute := self new handle: attributeAddress.					attribute shapeBlock						ifNotNil: 							[:block |							block								cull: (CairoContext basicNew setHandle: crAddress)								cull: attribute								cull: doPath ~= 0]]				ofType: LibPango PangoCairoShapeRendererFunc.	DestroyNotifyCallback := CCallback				do: [:data | self unregisterBlockAt: data]				ofType: LibPango GDestroyNotify.	DataCopyCallback := CCallback do: 					[:data |					self reregisterBlockAt: data.					data]				ofType: LibPango PangoAttrDataCopyFunc</body><body package="Pango">shapeRendererCallback	ShapeRendererCallback ifNil: [self initializeCallbacks].	^ShapeRendererCallback</body></methods><methods><class-id>Pango.ShapeAttribute class</class-id> <category>instance creation</category><body package="Pango">block: aBlock extents: aRectangle	^self block: aBlock ink: aRectangle logical: aRectangle</body><body package="Pango">block: aBlock ink: anInkRectangle logical: aLogicalRectangle	| ink logical blockIndex |	ink := PangoRectangle from: anInkRectangle.	logical := PangoRectangle from: aLogicalRectangle.	blockIndex := self registerBlock: aBlock.	^self new handle: (LibPango				pango_attr_shape_new_with_data: ink				with: logical				with: blockIndex				with: self dataCopyCallback				with: self destroyNotificationCallback)</body></methods><methods><class-id>Pango.TypeMask class</class-id> <category>values</category><body package="Pango">family	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r1</body><body package="Pango">stretch	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r10000</body><body package="Pango">style	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r10</body><body package="Pango">variant	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r100</body><body package="Pango">weight	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r1000</body></methods><methods><class-id>Pango.TypeMask class</class-id> <category>accessing</category><body package="Pango">gravity	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r1000000</body><body package="Pango">size	"http://library.gnome.org/devel/pango/unstable/pango-Fonts.html#PangoFontMask"	^2r100000</body></methods><methods><class-id>Pango.LayoutLine</class-id> <category>accessing</category><body package="Pango">byteLength	^(CIntegerType int newPointerToAddress: handle) at: 1</body><body packa