handle with: anIndentation toPangoScale</body><body package="Pango">justify	^(LibPango pango_layout_get_justify: handle) ~= 0</body><body package="Pango">justify: aBoolean	LibPango pango_layout_set_justify: handle with: aBoolean coerceToCInteger</body><body package="Pango">singleParagraphMode	^(LibPango pango_layout_get_single_paragraph_mode: handle) ~= 0</body><body package="Pango">singleParagraphMode: aBoolean	LibPango pango_layout_set_single_paragraph_mode: handle		with: aBoolean coerceToCInteger</body><body package="Pango">spacing	^(LibPango pango_layout_get_spacing: handle) fromPangoScale</body><body package="Pango">spacing: aNumber 	LibPango pango_layout_set_spacing: handle with: aNumber toPangoScale</body><body package="Pango">tabs	| tabsHandle |	tabsHandle := LibPango pango_layout_get_tabs: handle.	^tabsHandle isZero		ifTrue: [nil]		ifFalse: [TabArray new manageHandle: tabsHandle]</body><body package="Pango">tabs: aTabArray	LibPango pango_layout_set_tabs: handle with: (aTabArray ifNotNil: #handle)</body><body package="Pango">wrap	"returns a WrapMode value"	^LibPango pango_layout_get_wrap: handle</body><body package="Pango">wrap: aWrapMode	"e.g. aLayout wrap: WrapMode word"	LibPango pango_layout_set_wrap: handle with: aWrapMode</body></methods><methods><class-id>Pango.Layout</class-id> <category>deprecated</category><body package="Pango">pixelExtent	| widthPointer heightPointer |	self deprecated: #(#after '2007-07-25 21:09:05 GMT').	widthPointer := 0 copyToHeap.	heightPointer := 0 copyToHeap.	LibPango 		pango_layout_get_pixel_size: handle		with: widthPointer referentAddress		with: heightPointer referentAddress.	^[widthPointer contents @ heightPointer contents] ensure: 			[widthPointer free.			heightPointer free]</body><body package="Pango">pixelHeight	| heightPointer |	self deprecated: #(#after '2007-07-25 21:09:12 GMT').	heightPointer := 0 copyToHeap.	LibPango 		pango_layout_get_pixel_size: