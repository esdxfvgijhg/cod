ntents &gt; stop		or: [(LibPango pango_layout_iter_next_line: iterator) = 0]] whileFalse]		ensure:			[lineYstartPointer free.			lineYstopPointer free.			LibPango pango_layout_iter_free: iterator]</body></methods><methods><class-id>Pango.Layout</class-id> <category>accessing</category><body package="Pango">attributes	| attributesHandle |	attributesHandle := LibPango pango_layout_get_attributes: handle.	^attributesHandle isZero		ifTrue: [nil]		ifFalse: [AttributeList basicNew setHandle: attributesHandle]</body><body package="Pango">attributes: anAttributesList	LibPango pango_layout_set_attributes: handle with: anAttributesList handle</body><body package="Pango">baseline	^(LibPango pango_layout_get_baseline: handle) fromPangoScale</body><body package="Pango">context	^PangoContext basicNew		setHandle: (LibPango pango_layout_get_context: handle)</body><body package="Pango">contextChanged	LibPango pango_layout_context_changed: handle</body><body package="Pango">fontDescription	^FontDescription basicNew		setHandle: (LibPango pango_layout_get_font_description: handle)</body><body package="Pango">fontDescription: aFontDescription	LibPango pango_layout_set_font_description: handle		with: aFontDescription handle</body><body package="Pango">iterator	^Iterator new manageHandle: (LibPango pango_layout_get_iter: handle)</body><body package="Pango">markup: aString	"http://library.gnome.org/devel/pango/stable/PangoMarkupFormat.html"	| bytes |	bytes := aString utf8.	LibPango		pango_layout_set_markup: handle		with: bytes		with: bytes size</body><body package="Pango">text		^self utf8Bytes asStringEncoding: #UTF8</body><body package="Pango">text: aCharacterArray	| text start list |	text := aCharacterArray asText utf8.	self utf8Bytes: text string.	text isEmpty ifTrue: [^self].	(text runs values size = 1 and: [text runs values first isNil])		ifTrue: [^self].	start := 0.	list := AttributeList new.	text runs runs with: text runs values		do: 			[:run :emphasis |			(Attribute allFromEmphasis: emphasis) do: 					[:attribute |					attribute						startPosition: start;						endPosition: start + run.					list insert: attribute].			start := start + run].	self attributes: list</body><body package="Pango">utf8Bytes	| cPointer output byte |	cPointer := LibPango pango_layout_get_text: handle.	output := ByteArray new writeStream.	[(byte := cPointer contents) = 0] whileFalse: 			[output nextPut: byte.			cPointer += 1].	^output contents</body><body package="Pango">utf8Bytes: aUTF8ByteArray		LibPango		pango_layout_set_text: handle		with: aUTF8ByteArray		with: aUTF8ByteArray size</body></methods><methods><class-id>Pango.Layout</class-id> <category>helpers</category><body package="Pango">alignCenter	self alignment: Alignment center</body><body package="Pango">alignLeft	self alignment: Alignment left</body><body package="Pango">alignRight	self alignment: Alignment right</body><body package="Pango">fontDescriptionString: aString	self fontDescription: (FontDescription fromString: aString)</body></methods><methods><class-id>Pango.Layout</class-id> <category>lines</category><body package="Pango">lineAt: aLineOffset	"0 &lt;= aLineOffset &lt; self lineCount"	^LayoutLine new		handle: (LibPango pango_layout_get_line_readonly: handle with: aLineOffset)</body><body package="Pango">lineCount	^LibPango pango_layout_get_line_count: handle</body><body package="Pango">lineIndexFromPosition: aUTF8Offset	| result ignored line |	result := 0 copyToHeap.	ignored := 0 copyToHeap.	LibPango		pango_layout_index_to_line_x: handle		with: aUTF8Offset		with: 1		with: result		with: ignored.	line := result contents.	result free.	ignored free.	^line</body></methods><methods><class-id>Pango.Layout</class-id> <category>positioning</category><body package="Pango">positionFromPoint: aPoint	"Returns a zero based utf8 byte offset"	| byteIndexPointer byteIndex trailingPointer startPointer scanPointer |	byteIndexPointer := 0 copyToHeap.	trailingPointer := 0 copyToHeap.	LibPango		pango_layout_xy_to_index: handle		with: aPoint x toPangoScale		with: aPoint y toPangoScale		with: byteIndexPointer		with: trailingPointer.	byteIndex := byteIndexPointer contents.	startPointer := (LibPango pango_layout_get_text: handle) += byteIndex.	scanPointer := startPointer copy.	trailingPointer contents timesRepeat: 			[			[| byte |			scanPointer += 1.			byte := scanPointer contents.			byte &lt; 129 or: [byte &gt; 192]]					whileFalse].	^byteIndex + (scanPointer - startPointer)</body><body package="Pango">rectangleFromPosition: aUTF8Offset	| rectangle |	rectangle := PangoRectangle new.	LibPango pango_layout_index_to_pos: handle with: aUTF8Offset with: rectangle.	^rectangle asRectangle</body><body package="Pango">strongCursorRectangleFromPosition: aUTF8Offset	| rectangle |	rectangle := PangoRectangle new.	LibPango		pango_layout_get_cursor_pos: handle		with: aUTF8Offset		with: rectangle		with: nil.	^rectangle asRectangle</body></methods><methods><class-id>Pango.Layout</class-id> <category>testing</category><body package="Pango">isEllipsized	^(LibPango pango_layout_is_ellipsized: handle) ~= 0</body><body package="Pango">isWrapped	^(LibPango pango_layout_is_wrapped: handle) ~= 0</body></methods><methods><class-id>Pango.Layout class</class-id> <category>known handles</category><body package="Pango">knownHandles	^KnownLayouts</body></methods><methods><class-id>Pango.Layout class</class-id> <category>instance creation</category><body package="Pango">context: aPangoContext	^self new manageHandle: (LibPango pango_layout_new: aPangoContext handle)</body><body package="Pango">default	^self context: PangoContext default</body><body package="Pango">markup: aStringThing	"http://library.gnome.org/devel/pango/stable/PangoMarkupFormat.html"	^self default markup: aStringThing</body>