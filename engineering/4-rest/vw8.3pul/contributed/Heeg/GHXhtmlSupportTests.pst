lization code *** "	^self</body><body package="GHXhtmlSupportTests">testDifferences	self		checkOld: '&lt;p&gt;test&lt;/p&gt;'		new: '&lt;p&gt;test1&lt;/p&gt;'		produces: '&lt;p&gt;test&lt;ins&gt;1&lt;/ins&gt;&lt;/p&gt;'.	self		checkOld: '&lt;p&gt;test&lt;/p&gt;&lt;p&gt;test2&lt;/p&gt;'		new: '&lt;p&gt;test1&lt;/p&gt;&lt;p&gt;test&lt;/p&gt;'		produces: '&lt;p&gt;test&lt;ins&gt;1&lt;/ins&gt;&lt;/p&gt;&lt;p&gt;test&lt;del&gt;2&lt;/del&gt;&lt;/p&gt;'.</body><body package="GHXhtmlSupportTests">testDifferences1	self		checkOld: '&lt;p&gt;test&lt;/p&gt;'		new: '&lt;p&gt;test1&lt;/p&gt;&lt;p&gt;test&lt;/p&gt;'		produces: '&lt;p&gt;test&lt;ins&gt;1&lt;/ins&gt;&lt;/p&gt;&lt;p&gt;&lt;ins&gt;test&lt;/ins&gt;&lt;/p&gt;'</body><body package="GHXhtmlSupportTests">testDifferences3	self		checkOld: '&lt;p&gt;test&lt;/p&gt;'		new: '&lt;p&gt;test&lt;/p&gt;&lt;p&gt;test2&lt;/p&gt;'		produces: '&lt;p&gt;test&lt;/p&gt;&lt;p&gt;&lt;ins&gt;test2&lt;/ins&gt;&lt;/p&gt;'.	self		checkOld: 'test'		new: 'test&lt;p&gt;1&lt;/p&gt;'		produces: 'test&lt;p&gt;&lt;ins&gt;1&lt;/ins&gt;&lt;/p&gt;'.	self		checkOld: 'test&lt;p&gt;&lt;/p&gt;'		new: 'test&lt;p&gt;&lt;span&gt;a&lt;/span&gt;&lt;/p&gt;'		produces: 'test&lt;p&gt;&lt;span&gt;&lt;ins&gt;a&lt;/ins&gt;&lt;/span&gt;&amp;#13;&lt;/p&gt;'.	self		checkOld: '&lt;p&gt;test&lt;/p&gt;'		new: '&lt;h1&gt;test&lt;/h1&gt;'		produces: '&lt;p&gt;&lt;del&gt;test&lt;/del&gt;&lt;/p&gt;&lt;h1&gt;&lt;ins&gt;test&lt;/ins&gt;&lt;/h1&gt;'.</body><body package="GHXhtmlSupportTests">testDifferences4	self		checkOld: 'test&lt;span&gt;1&lt;/span&gt;'		new: 'test&lt;span&gt;1&lt;/span&gt;&lt;span&gt;2&lt;/span&gt;&lt;span&gt;3&lt;/span&gt;'		produces: 'test&lt;span&gt;1&lt;/span&gt;&lt;span&gt;&lt;ins&gt;2&lt;/ins&gt;&lt;/span&gt;&lt;span&gt;&lt;ins&gt;3&lt;/ins&gt;&lt;/span&gt;'.	self		checkOld: 'test&lt;span&gt;1&lt;/span&gt;'		new: 'test&lt;span&gt;3&lt;/span&gt;&lt;span&gt;2&lt;/span&gt;&lt;span&gt;1&lt;/span&gt;'		produces: 'test&lt;span&gt;&lt;del&gt;1&lt;/del&gt;&lt;ins&gt;3&lt;/ins&gt;&lt;/span&gt;&lt;span&gt;&lt;ins&gt;2&lt;/ins&gt;&lt;/span&gt;&lt;span&gt;&lt;ins&gt;1&lt;/ins&gt;&lt;/span&gt;'.</body></methods><methods><class-id>GHXhtmlCharacterDifferencesTests</class-id> <category>initialize-release</category><body package="GHXhtmlSupportTests">differencesBetween: oldString and: newString	^GHXhtmlDifferences characterDifferencesBetween: oldString and: newString</body></methods><methods><class-id>GHXhtmlCharacterDifferencesTests class</class-id> <category>instance creation</category><body package="GHXhtmlSupportTests">new	"Answer a newly created and initialized instance."	^super new initialize</body></methods><methods><class-id>GHXhtmlWordDifferencesTests</class-id> <category>initialize-release</category><body package="GHXhtmlSupportTests">differencesBetween: oldString and: newString	^GHXhtmlDifferences wordDifferencesBetween: oldString and: newString</body><body package="GHXhtmlSupportTests">initialize	"Initialize a newly created instance. This method must answer the receiver."	super initialize.	" *** Replace this comment with the appropriate initialization code *** "	^self</body><body package="GHXhtmlSupportTests">testDifferences	self		checkOld: '&lt;p&gt;test&lt;/p&gt;'		new: '&lt;p&gt;test1&lt;/p&gt;'		produces: '&lt;p&gt;&lt;del&gt;test&lt;/del&gt;&lt;ins&gt;test1&lt;/ins&gt;&lt;/p&gt;'.	self		checkOld: '&lt;p&gt;test&lt;/p&gt;&lt;p&gt;test2&lt;/p&gt;'		new: '&lt;p&gt;test1&lt;/p&gt;&lt;p&gt;test&lt;/p&gt;'		produces: '&lt;p&gt;&lt;del&gt;test&lt;/del&gt;&lt;ins&gt;test1&lt;/ins&gt;&lt;/p&gt;&lt;p&gt;&lt;del&gt;test2&lt;/del&gt;&lt;ins&gt;test&lt;/ins&gt;&lt;/p&gt;'.</body><body package="GHXhtmlSupportTests">testDifferences1	self		checkOld: '&lt;p&gt;test&lt;/p&gt;'		new: '&lt;p&gt;test1&lt;/p&gt;&lt;p&gt;test&lt;/p&gt;'		produces: '&lt;p&gt;&lt;del&gt;test&lt;/del&gt;&lt;ins&gt;test1&lt;/ins&gt;&lt;/p&gt;&lt;p&gt;&lt;ins&gt;test&lt;/ins&gt;&lt;/p&gt;'</body></methods><methods><class-id>GHXhtmlWordDifferencesTests class</class-id> <category>instance creation</category><body package="GHXhtmlSupportTests">new	"Answer a newly created and initialized instance."	^super new initialize</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>TestCase</name><environment>SUnit</environment><super>Core.Object</super><private>false</private><indexed-type>none</indexed-type><inst-vars>testSelector </inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>SUnit</category><attributes><package>SUnitToo</package></attributes></class></st-source>