 <category>accessing</category><body package="FormatedSetting">formatString	^formatString</body><body package="FormatedSetting">formatString: anObject	formatString := anObject</body></methods><methods><class-id>Heeg.FormatedSetting</class-id> <category>initialize-release</category><body package="FormatedSetting">editorModuleClassIn: aPage	^FormatedSettingModule</body></methods><methods><class-id>Heeg.FormatedSetting class</class-id> <category>instance creation</category><body package="FormatedSetting">formatString: aString min: minValue max: maxValue	| aFormatedSettings |	aFormatedSettings:=super new		min: minValue;		max: maxValue.	aFormatedSettings formatString:aString.	^aFormatedSettings</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>NumberSetting</name><environment>Tools</environment><super>Tools.SettingType</super><private>false</private><indexed-type>none</indexed-type><inst-vars>min max </inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>Tools-Settings</category><attributes><package>Tools-Settings</package></attributes></class><class><name>NumberSettingModule</name><environment>Tools</environment><super>Tools.ModularSettingsPageModule</super><private>false</private><indexed-type>none</indexed-type><inst-vars></inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>Tools-Settings-UI</category><attributes><package>Tools-Settings</package></attributes></class></st-source>