el release].	navigator removeDependent: self.	tools do: [:each | each release].	ShowStatusBar == nil ifFalse: [ShowStatusBar retractInterestsFor: self].	super release</body><body package="Browser-BrowserUI">resyncTools	(builder isNil or: [builder window isNil or: [builder window isOpen not]])		ifTrue: [^self].	self initTools.	self initStatusPanel.	self update</body></methods><methods><class-id>Refactory.Browser.AbstractCodeModel</class-id> <category>updating</category><body package="Browser-BrowserUI">changeRequest	^self updateRequest</body><body package="Browser-BrowserUI">firstDefaultTool	tabList list		do: [:each | each isDefaultSelectionCandidate ifTrue: [^each]].	^nil</body><body package="Browser-BrowserUI">selectDefaultTab	self selectTool: (self firstDefaultTool ifNil: [tabList list first])</body><body package="Browser-BrowserUI">update	"Tab labels can depend on tool state/content, so they must be updated afterwards."		self updateState.	self updateTabList.	self selectedTool updateContents.	self updateTabLabels.	self updateStatusPanel</body><body package="Browser-BrowserUI">update: anAspectSymbol with: aParameter from: aSender	anAspectSymbol = #closed ifTrue: [^self closeRequest].	lockPolicy isLocked ifTrue: [^self].	anAspectSymbol = #updateState ifTrue: [^self updateState].	self update</body><body package="Browser-BrowserUI">updateDisplay	"Suppress the generic implementation."</body><body package="Browser-BrowserUI">updateRequest	"Answer true if the receiver may be changed."		^self selectedTool notNil and: [self selectedTool updateRequest]</body><body package="Browser-BrowserUI">updateState	"We need to resync with the state."	self state: navigator getState</body><body package="Browser-BrowserUI">updateTabLabels	builder ifNotNil: [		(self tabBarPart widget)			resetVisuals;			invalidate]</body><body package="Browser-BrowserUI">updateTabList	"We watch for a change in the default tab to force an update of the selected tabs, otherwise, if it's still present, maintain the selection."	| list selected previousDefault newDefault |	previousDefault := self firstDefaultTool.	selected := self selectedTool.	list := self activeTools.	list = tabList list		ifFalse: 			[self tabList list: list.			newDefault := self firstDefaultTool.			((list includes: selected) and: [previousDefault = newDefault])				ifTrue: [self selectTool: selected]				ifFalse: [self selectDefaultTab]]</body></methods><methods><class-id>Refactory.Browser.AbstractCodeModel</class-id> <category>status panel</category><body package="Browser-BrowserUI">initStatusPanel	statusPanel == nil ifTrue: [^self].	statusPanel pragmas: self</body><body package="Browser-BrowserUI">newStatusPanel		statusPanel ifNotNil: #release.	statusPanel := ToolBarView new.	statusPanel variantName: #statusbar.	statusPanel pragmas: self.	^BoundedWrapper on: statusPanel</body><body package="Browser-BrowserUI">updateStatusPanel	statusPanel == nil ifTrue: [^self].	(statusPanel components collect: #widget) do: [:each |		(each respondsTo: #touch) ifTrue: [each touch]]</body></methods><methods><class-id>Refactory.Browser.AbstractCodeModel</class-id> <category>menu accessing</category><body package="Browser-BrowserUI">menu	self selectedTool ifNil: [^Menu new].	^self selectedTool menu</body></methods><methods><class-id>Refactory.Browser.AbstractCodeModel</class-id> <category>navigator testing</category><body package="Browser-BrowserUI">isMeta	^self state isMeta</body></methods><methods><class-id>Refactory.Browser.AbstractCodeModel</class-id> <category>subcanvases</category><body package="Browser-BrowserUI">saveState	self selectedTool ifNotNil: [:tool | tool saveState]</body></methods><methods><class-id>Refactory.Browser.AbstractCodeModel</class-id> <category>testing</category><body package="Browser-BrowserUI">isDirty	^false</body><body package="Browser-BrowserUI">isLocked	^lock