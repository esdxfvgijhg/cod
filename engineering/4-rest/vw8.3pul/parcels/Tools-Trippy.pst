ue:		[(aSubmenu atNameKey: #exploreFocus) beOn].	installedNavigator vicinity == #owner ifTrue:		[(aSubmenu atNameKey: #exploreSiblings) beOn].	self visit hasOwner ifFalse:		[(aSubmenu atNameKey: #exploreSiblings) disable].	installedNavigator vicinity == #self ifTrue:		[(aSubmenu atNameKey: #exploreParts) beOn].	self inspector isPartList ifFalse:		[(aSubmenu atNameKey: #exploreParts) disable].	installedNavigator vicinity == #visited ifTrue:		[(aSubmenu atNameKey: #exploreVisited) beOn].	self addExploreItemsTo: aSubmenu.	^aSubmenu</body><body package="Tools-Trippy">buildGoSubmenu	| menu |	menu := self class baseGoSubmenu.	(menu atNameKey: #goDive)		visibility: [self canDive];		submenu: [self inspector buildDiveMenu].	(menu atNameKey: #goBack) enabled: [self canGoBack].	(menu atNameKey: #goForward) enabled: [self canGoForward].	self addCollaboratorItemsTo: menu.	^menu</body><body package="Tools-Trippy">buildHistorySubmenu	| menu item |	menu := Menu new.	trail keysAndValuesDo:		[:index :navigator | | label |		label := index &lt; 10			ifTrue: ['&amp;', index printString, '. ', navigator visit displayString]			ifFalse: [navigator visit displayString].		item := MenuItem labeled: label.		item value: [self goTo: index].		index = current ifTrue: [item beOn].		menu addItem: item].	^menu</body><body package="Tools-Trippy">buildMenuBar	| menu |	menu := self class menuBar.	self hasCurrentInspector ifTrue:		[self inspector augmentMenuBar: menu].	(menu atNameKey: #go) submenu: self buildGoSubmenu.	(menu atNameKey: #history) submenu: self buildHistorySubmenu.	(menu atNameKey: #explore) submenu: self buildExploreSubmenu.	(menu atNameKey: #toggleToolbar) indication: [self builder window toolbarVisible].	(menu atNameKey: #toggleEvaluationPane) indication: [evaluationPaneVisible].	[menu		augmentFrom: self class		to: InspectorWindow		menuName: #menuBar		for: self]			on: MenuAugmentationError			do: [:ex | ex isResumable ifTrue: [ex resume]].	^menu</body><body package="Tools-Trippy">refreshMenuBar	menuBarHolder value: self buildMenuBar</body></methods><methods><class-id>Tools.Trippy.InspectorWindow</class-id> <category>private-evaluation</category><body package="Tools-Trippy">doItContext	"Answer the context in which a text selection can be evaluated."	^nil</body><body package="Tools-Trippy">doItEnvironment	"Answer the environment in which context the text selection is evaluated."	^self doItReceiverClass environment</body><body package="Tools-Trippy">doItExpectPattern	^false</body><body package="Tools-Trippy">doItReceiver	"Answer the object in which context the text selection is evaluated."	^self object</body><body package="Tools-Trippy">doItReceiverClass	"Answer the object class in which context the text selection is evaluated."	^self doItReceiver class</body><body package="Tools-Trippy">evaluator	^self widgetAt: #evaluator</body></methods><methods><class-id>Tools.Trippy.InspectorWindow</class-id> <category>actions</category><body package="Tools-Trippy">copySelection	self evaluator hasFocus		ifTrue: [self evaluator clipboardCopy]		ifFalse: [self inspector copySelection]</body><body package="Tools-Trippy">helpAbout	AboutVisualWorksDialog open</body><body package="Tools-Trippy">helpInspectingObjects	'topic:vw.help.inspect' asURI open</body><body package="Tools-Trippy">helpInspector	'topic:vw.help.tools.inspector' asURI open</body><body package="Tools-Trippy">paste	self evaluator hasFocus		ifTrue: [self evaluator clipboardPaste]		ifFalse: [self inspector paste]</body><body package="Tools-Trippy">selectAllFields	self evaluator hasFocus		ifTrue: [self evaluator selectAll]		ifFalse: [self inspector selectAllFields]</body><body package="Tools-Trippy">toggleEvaluationPane 	evaluationPaneVisible := evaluationPaneVisible not.	EvaluationPaneWasLastShown := evaluationPaneVisible.	self setEvaluatorLayout.	builder window refresh.